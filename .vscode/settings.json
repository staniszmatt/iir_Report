{
  "files.associations": {
    ".babelrc": "jsonc",
    ".eslintrc": "jsonc",
    ".prettierrc": "jsonc",
    ".stylelintrc": "json",
    ".dockerignore": "ignore",
    ".eslintignore": "ignore"
  },

  "javascript.validate.enable": false,
  "javascript.format.enable": false,
  "typescript.format.enable": false,

  "search.exclude": {
    ".git": true,
    ".eslintcache": true,
    "app/dist": true,
    "app/main.prod.js": true,
    "app/main.prod.js.map": true,
    "bower_components": true,
    "dll": true,
    "release": true,
    "node_modules": true,
    "npm-debug.log.*": true,
    "test/**/__snapshots__": true,
    "yarn.lock": true,
    "*.{css,sass,scss}.d.ts": true
  },
  "cSpell.words": [
    "Amila",
    "INCO",
    "PARTNUMBERS",
    "Rangaraj",
    "Vikram",
    "Welihinda",
    "browserslist",
    "compat",
    "customerlist",
    "develar",
    "devtron",
    "eslintignore",
    "esnext",
    "fbjs",
    "fontawesome",
    "nsis",
    "opencollective",
    "postlint",
    "preinstall",
    "prestart",
    "prettierrc",
    "spectron",
    "stylelintrc",
    "submenu",
    "tapable",
    "testcafe",
    "unhide",
    "vfile",
    "webm",
    "webp",
    "woff"
  ]
}
